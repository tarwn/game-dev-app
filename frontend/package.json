{
  "name": "gdp-frontend",
  "version": "1.0.0",
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "routify -c dev:server:watch --",
    "dev:server:watch": "rollup -c -w",
    "dev:server:start": "sirv dist --single",
    "validate": "svelte-check --fail-on-warnings",
    "lint": "eslint ./src/**/*.ts",
    "test": "jest src",
    "test:watch": "yarn run test -- --watch",
    "ci:validate": "svelte-check --fail-on-warnings --output machine",
    "ci:lint": "eslint ./src/**/*.ts --format junit",
    "ci:test": "jest src --reporters=jest-junit",
    "icon": "./node_modules/.bin/icon-font-generator ./resources/font/*.svg -n UiconMAX -o ./public/fonts/ --csspath ./src/styles/uiconmax.css --cssfontsurl /fonts -p --htmlpath ./resources/font/preview.html uiconmax --descent 4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "autoprefixer": "^10.0.1",
    "d3": "^6.3.1",
    "del": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-editorconfig": "^2.1.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "icon-font-generator": "^2.1.11",
    "immer": "^8.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nock": "^13.0.5",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.27.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.1.5",
    "svelte-portal": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "yargs": "^16.0.3"
  },
  "dependencies": {
    "@microsoft/signalr": "^5.0.0",
    "@sentry/browser": "^5.26.0",
    "@sentry/integrations": "^5.27.6",
    "@sveltech/routify": "^1.9.9",
    "sirv-cli": "^1.0.0"
  },
  "jest-junit": {
    "includeConsoleOutput": "true",
    "outputName": "frontend.xml",
    "ancestorSeparator": " > ",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{classname} > {title}"
  }
}
