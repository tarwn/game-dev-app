{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "html.autoClosingTags": false,
  "emmet.showExpandedAbbreviation": "never",
  "emmet.excludeLanguages": [
    "markdown",
    "svelte"
  ]
}
